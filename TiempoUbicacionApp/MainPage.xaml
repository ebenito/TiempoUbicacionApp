<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TiempoUbicacionApp"
             xmlns:sharedComponents="clr-namespace:TiempoUbicacion.Shared.Components;assembly=TiempoUbicacion.Shared"
             x:Class="TiempoUbicacionApp.MainPage"
             xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type sharedComponents:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
